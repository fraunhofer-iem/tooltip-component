{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "module": "es6",
      "moduleResolution": "node",
      "target": "es5",
      "allowJs": true,
      "isolatedModules": true,
      "jsx": "react",
      "esModuleInterop" : true,
      "allowSyntheticDefaultImports" : true,
      "declaration": true,
      "noEmit": false
  }
}